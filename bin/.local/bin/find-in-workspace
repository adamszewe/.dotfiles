#!/usr/bin/env bash

echo "work in progress... this feature is not ready yet"
found_item=$(find ~/Documents ~/ ~/Documents/workspace -maxdepth 2 -mindepth 1 -type d 2> /dev/null | fzf)
if [ -z "$found_item" ]; then
    exit 0
else
    echo "opening the directory: $found_item"
    cd "$found_item"
fi
